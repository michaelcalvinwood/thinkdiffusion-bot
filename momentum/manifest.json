{
    "manifest_version": 2,
    "name": "Think Diffusion Bot",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.thinkdiffusion.com/*"],
        "js": ["thinkdiffusion.js"]
      }
    ],
    "applications": {
      "gecko": {
        "id": "thinkdiffusion-bot@appgalleria.com"
      }
    },
    "web_accessible_resources": [
      "prompts.json"
    ]
  }
  